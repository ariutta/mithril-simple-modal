{
  "name": "mithril-simple-modal",
  "version": "0.0.0",
  "description": "Mithril wrapper for wunderlink/simple-modal.",
  "main": "index.js",
  "scripts": {
    "postinstall": "node ./node_modules/browserify/bin/cmd.js ./node_modules/simple-modal/index.js --standalone createSimpleModal | ./node_modules/derequire/bin/cmd.js > ./node_modules/simple-modal/simple-modal.compiled.js",
    "start": "./node_modules/http-server/bin/http-server",
    "test": "node ./node_modules/mocha/bin/mocha ./test/unit/api.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ariutta/mithril-simple-modal.git"
  },
  "keywords": [
    "mithril",
    "modal",
    "lightbox",
    "light-box",
    "simple-modal"
