  "author": "Anders Riutta",
  "license": "BSD",
  "bugs": {
    "url": "https://github.com/ariutta/mithril-simple-modal/issues"
  },
  "homepage": "https://github.com/ariutta/mithril-simple-modal",
  "dependencies": {
    "browserify": "^12.0.1",
    "coffeeify": "^2.0.1",
    "cssify": "^1.0.1",
    "derequire": "^2.0.3",
    "isvisible": "^1.0.6",
    "jade": "^1.11.0",
    "jade-runtime": "^2.0.0",
    "jadeify": "^4.5.0",
    "mithril": "^0.2.0",
    "simple-modal": "^1.0.1",
    "spin.js": "^2.0.2"
  },
  "devDependencies": {
    "chai": "^3.4.1",
    "http-server": "^0.8.5",
    "jquery": "^1.11.3",
    "jsdom": "^7.2.1",
    "mocha": "^2.3.4",
    "mocha-jsdom": "^1.0.0",
    "rx": "^2.5.3"
  }
}
